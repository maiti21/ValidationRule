/* Polygonalized pier should be with area tag;*/

way[man_made=pier]:closed[area!=yes]{
  throwWarning: tr("Pier in polygon, missing area=yes");
  group: tr("missing tag combination in Pier");
  fixAdd: "area=yes";
}

way[man_made=breakwater]:closed[area!=yes]{
  throwWarning: tr("Breakwater in polygon, missing area=yes");
  group: tr("missing tag combination in Breakwater");
  fixAdd: "area=yes";
}